{
  "name": "blog.appleseed.dev",
  "private": true,
  "description": "A personal blog",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/apple12347678/blog.appleseed.dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "tsc": "tsc",
    "lint": "eslint --cache src",
    "lint:strict": "yarn lint --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "yarn format --check",
    "format:write": "yarn format --write",
    "refactor": "yarn tsc && yarn lint:fix && yarn format:write",
    "create-article": "TS_NODE_PROJECT=script/tsconfig.json yarn ts-node script/create-article.ts",
    "clean-start": "yarn clean && yarn build && yarn serve"
  },
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@gatsbyjs/reach-router": "^1.3.6",
