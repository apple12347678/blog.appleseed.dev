{
  "name": "blog.appleseed.dev",
  "private": true,
  "description": "A personal blog",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/apple12347678/blog.appleseed.dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "tsc": "tsc",
    "lint": "eslint --cache src",
    "lint:strict": "yarn lint --max-warnings=0",
    "lint:fix": "yarn lint --fix",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "yarn format --check",
    "format:write": "yarn format --write",
    "refactor": "yarn tsc && yarn lint:fix && yarn format:write",
    "create-article": "TS_NODE_PROJECT=script/tsconfig.json yarn ts-node script/create-article.ts",
    "clean-start": "yarn clean && yarn build && yarn serve"
  },
  "dependencies": {
    "@babel/core": "^7",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "gatsby": "^2.24.80",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-emotion": "^6.0.0",
    "gatsby-plugin-feed": "^2.5.15",
    "gatsby-plugin-google-analytics": "^2.3.19",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-layout": "^1.3.13",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-react-i18next": "^0.0.27",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.6.42",
    "gatsby-plugin-typescript": "^2.4.22",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-autolink-headers": "^2.3.15",
    "gatsby-remark-copy-linked-files": "^2.3.19",
    "gatsby-remark-footnotes": "^0.0.7",
    "gatsby-remark-images": "^3.3.35",
    "gatsby-remark-katex": "^3.3.14",
    "gatsby-remark-prismjs": "^3.5.16",
    "gatsby-remark-responsive-iframe": "^2.4.17",
    "gatsby-remark-smartypants": "^2.3.13",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-transformer-remark": "^2.8.42",
    "gatsby-transformer-sharp": "^2.5.18",
    "graphql": "^14",
    "i18next": "^19.8.2",
    "katex": "^0.12.0",
    "lodash": "^4.17.20",
    "prismjs": "^1.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "regenerator-runtime": "^0.13.7",
    "remark-parse": "^9.0.0",
    "svg-react-loader": "^0.4.6",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.22",
    "@types/reach__router": "^1.3.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.13",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.3",
    "yaml": "^1.10.0",
    "yargs": "^16.1.0"
  }
}
