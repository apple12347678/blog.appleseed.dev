    "gatsby": "^3.1.2",
    "gatsby-image": "^3.1.0",
    "gatsby-legacy-polyfills": "^1.1.0",
    "gatsby-link": "^3.1.0",
    "gatsby-plugin-emotion": "^6.1.0",
